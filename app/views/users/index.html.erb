<h1>Users#index</h1>

<table class="table">
  <thead>
    <tr>
      <th><%= User.human_attribute_name :username %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th><%= user.username %></th>
      </tr>
    <% end %>
  </tbody>
</table>

<% if logged_in? %>
  <%= button_to 'ログアウト', logout_path, method: :delete %>
<% else %>
  <%= link_to 'ログイン', login_path %>
<% end %>
